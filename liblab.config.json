{
  "sdkName": "PlexSDK",
  "specFilePath": "./plex-media-server-spec-dereferenced.yaml",
  "languages": ["python", "typescript", "java"],
  "createDocs": true,
  "auth": ["apikey"],
  "customizations": {
    "authentication": {
      "apiKey": {
        "header": "X-Plex-Token"
      }
    },
    "generateEnv": true,
    "devContainer": true,
    "license": {
      "type": "MIT"
    },
    "retry": {
      "enabled": true,
      "retryDelay": 150,
      "maxAttempts": 3
    },
    "injectedModels": [],
    "inferServiceNames": false,
    "responseHeaders": true
  },
  "languageOptions": {
    "typescript": {
      "npmName": "plexjs",
      "npmOrg": "lukehagar",
      "httpClient": "axios",
      "bundle": false,
      "exportClassDefault": false,
      "githubRepoName": "plexjs",
      "sdkVersion": "0.0.1",
      "ignoreFiles": [],
      "authors": [
        {
          "name": "Luke Hagar",
          "email": "lukeslakemail@gmail.com"
        }
      ]
    },
    "python": {
      "pypiPackageName": "plexpy",
      "alwaysInitializeOptionals": false,
      "githubRepoName": "plexpy",
      "sdkVersion": "0.0.1",
      "ignoreFiles": [],
      "authors": [
        {
          "name": "Luke Hagar",
          "email": "lukeslakemail@gmail.com"
        }
      ]
    },
    "java": {
      "githubRepoName": "plexjava",
      "sdkVersion": "0.0.1",
      "ignoreFiles": [],
      "java": {
        "groupId": "sdk.plex"
      },
      "authors": [
        {
          "name": "Luke Hagar",
          "email": "lukeslakemail@gmail.com"
        }
      ]
    }
  },
  "publishing": {
    "githubOrg": "lukehagar"
  }
}
